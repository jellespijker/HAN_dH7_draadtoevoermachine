*.llb mergetool=labview merge=labview
*.ctl diff=labview difftool=labview
*.ctt diff=labview difftool=labview
*.mlx -crlf -diff -merge
*.mat -crlf -diff -merge
*.fig -crlf -diff -merge
*.p -crlf -diff -merge
*.slx -crlf -diff -merge
*.mdl -crlf -diff -merge
